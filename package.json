    "jszip": "^3.10.1"
  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "html-minifier": "^4.0.0",
    "markdown-it-attrs": "^4.2.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^4.0.0",
    "npm-run-all": "^4.1.5"
